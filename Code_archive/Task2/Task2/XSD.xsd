<?xml version="1.0" encoding="utf-8" ?>
<xs:schema id="Bibl" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="Biblio">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="Book" maxOccurs="unbounded">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="Title" type="xs:string"/>
              <xs:element name="Author" type="xs:string"/>
              <xs:element name="Binding" type="xs:string"/>
              <xs:element name="Pages" type="xs:integer"/>
              <xs:element name="Prise" type="xs:decimal"/>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
</xs:schema>